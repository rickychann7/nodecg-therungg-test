{
	"name": "nodecg-therungg-test",
	"version": "1.0.0",
	"scripts": {
		"dev": "run-p dev:*",
		"dev:tsc": "tsc -b src/browser src/extension -w --preserveWatchOutput",
		"dev:vite": "vite",
		"dev:nodecg": "node --enable-source-maps ../..",
		"generate-schema-types": "trash src/types/schemas && nodecg schema-types",
		"build": "vite build"
	},
	"nodecg": {
		"compatibleRange": "^2.0.0",
		"dashboardPanels": [
			{
				"name": "therungg",
				"title": "therun.gg",
				"width": 2,
				"file": "therungg.html",
				"workspace": "DASHBOARD",
				"headerColor": "#525F78"
			}
		],
		"graphics": [
			{
				"file": "example.html",
				"width": 1920,
				"height": 1080,
				"singleInstance": false
			}
		]
	},
	"devDependencies": {
		"@emotion/react": "^11.13.3",
		"@emotion/styled": "^11.13.0",
		"@mdi/react": "^1.6.1",
		"@mui/material": "^6.1.1",
		"@nodecg/react-hooks": "^1.0.3",
		"@nodecg/types": "^2.1.12",
		"@types/react": "^18.3.8",
		"@types/react-dom": "^18.3.0",
		"@vitejs/plugin-react": "^4.3.1",
		"globby": "^14.0.2",
		"npm-run-all": "^4.1.5",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"rollup-plugin-esbuild": "^6.1.1",
		"rollup-plugin-node-externals": "^7.1.3",
		"trash-cli": "^6.0.0",
		"typescript": "^5.6.2",
		"vite": "^5.4.8"
	},
	"dependencies": {
		"therungg": "^1.2.2"
	}
}
